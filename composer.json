{
  "name": "jonesiscoding/pleasing-filters",
  "description": "Pleasing and Assetic  filters for prefixing CSS, compiling of SCSS, and minification of and JS assets.",
  "keywords": [
      "sass",
      "scss",
      "php",
      "assetic",
      "pleasing",
      "sassc",
      "minify",
      "js",
      "css",
      "prefix",
      "vendor prefix"
  ],
  "homepage": "https://www.github.com/jonesiscoding/pleasing-filters",
  "version": "3.1",
  "authors": [
      {
          "name": "Aaron M Jones",
          "homepage": "http://www.jonesiscoding.com"
      }
  ],
  "license": "MIT",
  "autoload": {
      "psr-4": {
          "DevCoding\\Pleasing\\Filters\\": "src"
      }
  },
  "require": {
      "tedivm/jshrink": "~1.1",
      "jonesiscoding/xq-sassy": "^2.2"
  },
  "replace": {
    "exactquery/pleasing-filters": "^1.0"
  },
  "suggest": {
    "jonesiscoding/pleasing": "For serving and caching assets processed by Assetic."
  },
  "require-dev": {
    "kriswallsmith/assetic": "~1.3"
  }
}
